        <activity android:name=".Activitys.AddActivity" />
        <activity android:name=".Activitys.DetalleActivity" />
        <activity android:name=".Activitys.ProductosActivity" />
        <activity android:name=".Activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.MenuActivity" />
        <activity android:name=".Activitys.RegisterActivity" />
        <activity android:name=".Activitys.LoginActivity" />
        <activity android:name=".Activitys.MainActivity" />
