        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activitys.GestionPedidosAdminActivity"></activity>
        <activity android:name=".Activitys.DetalleVentaActivity" />
